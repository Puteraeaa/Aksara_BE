{
  "name": "express-mysql-migration-app",
  "version": "1.0.0",
  "description": "An Express.js application with MySQL and migration setup for user management.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migration:create": "npx sequelize-cli migration:generate --name"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.4.4"
  },
  "author": "",
  "license": "MIT"
}